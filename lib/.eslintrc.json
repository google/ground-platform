{
  "extends": "./node_modules/gts",
  "env": {
    "jasmine": true
  },
  "parser": "@typescript-eslint/parser",
  "root": true,
  "rules": {
    "eqeqeq": ["error", "always", {"null": "ignore"}],
    "object-curly-spacing": ["error", "always"],
    "n/no-extraneous-require": ["error", {
      "allowModules": ["firebase-admin"]
    }]
  }
}
