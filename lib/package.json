{
  "name": "@ground/lib",
  "version": "0.0.1",
  "main": "dist/index.js",
  "description": "Ground shared lib",
  "scripts": {
    "clean": "rm -rf dist *.log src/generated",
    "lint": "eslint --ext .js,.ts src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "jasmine --config=jasmine.json",
    "ci-all": "npm run ci-local-deps && npm run build-local-deps && npm ci",
    "ci-local-deps": "npm ci --workspace=../proto",
    "build-local-deps": "npm run build --workspace=../proto",
    "build": "tsc"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "dependencies": {
    "@google-cloud/firestore": "^7.7.0",
    "@ground/proto": "file:../proto",
    "firebase-functions": "^5.0.1",
    "immutable": "^4.3.6",
    "protobufjs": "^7.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "^4.3.5",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^5.3.0",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "4.8.4"
  }
}
