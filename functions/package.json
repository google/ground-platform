{
  "name": "functions",
  "main": "lib/index.js",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "clean": "rm -rf node_modules lib *.log",
    "lint": "eslint --ext .js,.ts src/",
    "lint:fix": "npm run lint -- --fix",
    "shell": "firebase functions:shell",
    "emulators": "firebase emulators:start --only functions",
    "login": "firebase login",
    "deploy": "npm run build && firebase deploy --only functions:profile.refresh --project",
    "logs": "firebase functions:log",
    "test": "npm run lint && mocha --reporter list",
    "build": "npm install && tsc",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@fast-csv/format": "4.3.5",
    "@terraformer/wkt": "2.1.2",
    "busboy": "1.5.0",
    "cors": "2.8.5",
    "csv-parser": "2.3.3",
    "firebase-admin": "11.5.0",
    "firebase-functions": "^4.1.1",
    "google-auth-library": "6.1.3",
    "googleapis": "64.0.0",
    "http-status-codes": "1.4.0",
    "jsonstream-ts": "1.3.6",
    "module-alias": "^2.2.2",
    "requests": "0.3.0",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": "16"
  },
  "private": true,
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/cors": "2.8.12",
    "@types/jsonstream": "0.8.30",
    "@types/node": "18.7.13",
    "@types/terraformer__wkt": "2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-absolute-imports": "^0.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-tools": "11.23.1",
    "gts": "^3.1.1",
    "mocha": "10.0.0",
    "prettier": "^2.8.1",
    "typescript": "4.8.4",
    "wkt": "0.1.1"
  },
  "_moduleAliases": {
    "@": "lib"
  }
}
