{
  "name": "@ground/functions",
  "version": "0.0.1",
  "main": "dist/index.js",
  "description": "Cloud Functions for Firebase",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist *.log",
    "lint": "eslint src/",
    "shell": "firebase functions:shell",
    "emulate": "firebase emulators:start --only functions,firestore --project demo-local",
    "logs": "firebase functions:log",
    "test": "jasmine --config=jasmine.json",
    "build": "tsc",
    "deploy": "npm run build && firebase deploy --project $npm_config_project --only functions",
    "watch": "tsc --watch"
  },
  "nx": {
    "name": "functions",
    "targets": {
      "test": {
        "commands": "jasmine --config=jasmine.json",
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "dependencies": {
    "@fast-csv/format": "4.3.5",
    "@ground/lib": "workspace:*",
    "@ground/proto": "workspace:*",
    "@terraformer/wkt": "2.1.2",
    "busboy": "1.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.5",
    "firebase-admin": "catalog:",
    "firebase-functions": "catalog:",
    "geojson": "0.5.0",
    "google-auth-library": "6.1.3",
    "googleapis": "64.0.0",
    "immutable": "catalog:",
    "http-status-codes": "1.4.0",
    "jsonstream-ts": "1.3.6",
    "module-alias": "^2.2.2",
    "nodemailer": "^7.0.11",
    "sanitize-html": "^2.13.1"
  },
  "engines": {
    "node": "20"
  },
  "private": true,
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "2.8.12",
    "@types/geojson": "^7946.0.14",
    "@types/jasmine": "catalog:dev",
    "@types/node": "catalog:dev",
    "@types/nodemailer": "^6.4.16",
    "@types/sanitize-html": "^2.13.0",
    "@types/terraformer__wkt": "2.0.0",
    "firebase-functions-test": "^3.3.0",
    "form-data-encoder": "^4.0.2",
    "formdata-node": "^6.0.3",
    "isolate-package": "^1.18.0"
  },
  "type": "commonjs"
}
