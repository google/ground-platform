{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "multi-polygon-object.json",
  "title": "MultiPolygonObject",
  "type": "object",
  "required": [
    "type",
    "coordinates"
  ],
  "properties": {
    "type": {
      "const": "MultiPolygon",
      "default": "MultiPolygon"
    },
    "coordinates": {
      "type": "array",
      "items": {
        "$ref": "polygon-coordinates-object.json"
      },
      "minItems": 1
    }
  },
  "additionalProperties": false
}