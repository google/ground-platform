{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "geo-point-object.json",
  "$comment": "A pair of web mercator coordinates, represented as a GeoPoint in Firebase.",
  "title": "GeoPointObject",
  "type": "object",
  "x-kt-type": "GeoPoint",
  "required": ["latitude", "longitude"],
  "properties": {
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    }
  },
  "additionalProperties": false
}