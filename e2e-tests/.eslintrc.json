{
  "extends": "../node_modules/gts",
  "env": {
    "jasmine": true
  },
  "parser": "@typescript-eslint/parser",
  "root": true,
  "rules": {
    "node/no-unpublished-import": ["error", {
      "allowModules": ["jasmine"]
    }],
    "eqeqeq": ["error", "always", {"null": "ignore"}],
    "object-curly-spacing": ["error", "always"]
  }
}
