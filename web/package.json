{
  "name": "ground",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf node_modules dist coverage *.log",
    "start:dev": "npm run build:dev --project=$npm_config_project && ng serve -c dev",
    "start:prod": "npm run build:prod --project=$npm_config_project && ng serve -c production",
    "build": "npm install && ng build -c",
    "update-web-keys": "npm run --prefix scripts update-web-keys",
    "build:dev": "npm run update-web-keys $npm_config_project && npm run build -- dev",
    "build:prod": "npm run update-web-keys $npm_config_project && npm run build -- production",
    "build:local": "npm run build local",
    "watch:local": "npm run build local -- --watch",
    "pretest": "./pretest.sh",
    "test": "ng test",
    "test:headless": "ng test --browsers ChromeHeadlessNoSandbox --watch=false",
    "test-with-coverage": "npm run pretest && npm run test:headless -- --code-coverage && npm run posttest",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "posttest": "./posttest.sh && npm run lint",
    "login": "npx firebase login",
    "deploy:dev": "npm run build:dev --project=$npm_config_project && npm run login && npx firebase deploy --only hosting --project $npm_config_project"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.2.9",
    "@angular/google-maps": "^15",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@grpc/grpc-js": "^1.8.8",
    "@iplab/ngx-file-upload": "^15",
    "firebase": "^9.8.3",
    "firebaseui": "^6.0.1",
    "firebaseui-angular": "^6.1.2",
    "functions": "^1.0.9",
    "hammerjs": "^2.0.8",
    "immutable": "^4.0.0-rc.12",
    "ngx-autosize-input": "^15",
    "ngx-color": "^5.1.4",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.1",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@types/ground-schema": "file:../schema/dist/generated",
    "@angular-devkit/architect": "0.1502.9",
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "^15",
    "@angular-eslint/eslint-plugin-template": "^15.1.0",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@jridgewell/source-map": "^0.3.2",
    "@types/googlemaps": "^3.40.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasmine-expect": "3.8.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.11.2",
    "@types/webpack-env": "^1.15.3",
    "codelyzer": "^6.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-tools": "11.23.1",
    "fuzzy": "^0.1.3",
    "gts": "^4.0.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "istanbul": "^0.4.5",
    "jasmine-core": "^4.5.0",
    "jasmine-matchers": "0.2.3",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-jasmine-matchers": "5.0.0",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "puppeteer": "^3.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.9.0"
  }
}
